<?php

/**
 * Implements hook_enable()
 */
function os_landing_page_enable(){
  //Change default frontpage
  $value = 'os/frontpage';
  variable_set('site_frontpage', $value);
}

/**
 * Implements hook_disable()
 */
function os_landing_page_disable(){
  //Change default frontpage
  $value = 'home';
  variable_set('site_frontpage', $value);
}
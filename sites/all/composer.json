{
  "name": "OpenScholarLLC",
  "require": {
    "openscholar/osllc" : "dev-saml_support@dev"
  },
  "repositories": {
    "0": {
      "type": "vcs",
      "url": "git@bitbucket.org:openscholar/osllc.git"
    },
    "drupal": {
        "type": "composer",
        "url": "https://packages.drupal.org/7"
    }
  
  },
  "config": {
    "vendor-dir": "~/src/amazon/web/sites/all/modules",
    "discard-changes": true
  },
  "minimum-stability": "dev",
  "prefer-stable": "true"
}